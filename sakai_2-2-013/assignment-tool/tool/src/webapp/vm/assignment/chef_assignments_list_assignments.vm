							<td headers="openDate">
								$!assignment.openTime.toStringLocalFull()
							</td>
							<td headers="dueDate">
								$!assignment.dueTime.toStringLocalFull()
							</td>
							#if ($!allowGradeSubmission)
								#if ($!view.equals('lisofass1'))
								<td headers="num_submissions" class="specialLink">
									#set ($totalSubmission = $service.getSubmissions($assignment))
									#set ($totalNumber = 0)
									#set ($ungradedNumber = 0)
									#foreach ($s in $totalSubmission)
										#if ($s.Submitted)
											#set ($totalNumber = $totalNumber + 1)
											#if (!$s.Graded)
												#set ($ungradedNumber = $ungradedNumber + 1)
											#elseif ($s.Returned)
												#set ($returnedTime = $s.getTimeReturned())
												#set ($submitTime = $s.getTimeSubmitted())
												#if ($!returnedTime && $returnedTime.before($submitTime))
													## has been resubmitted, and ungraded
													#set ($ungradedNumber = $ungradedNumber + 1)
												#end
											#end
										#end
									#end
									<a href="#" onclick="location='#toolLinkParam("$action" "doGrade_assignment_from" "assignmentId=$validator.escapeUrl($assignmentReference)")'; return false;" onkeypress="location='#toolLinkParam("$action" "doGrade_assignment_from" "assignmentId=$validator.escapeUrl($assignmentReference)")'; return false;">
									$totalNumber/$ungradedNumber</a>
								</td>
								#end
							#end
							#if ($withGrade && $!allowGradeSubmission)
								<td headers="maxgrade">
									#set ($gradeScale = $assignment.getContent().getTypeOfGrade())
									#if ($gradeScale == -1)
										## Grade type not set 
										$tlang.getString("gen.noset")
									#elseif ($gradeScale == 1)
										## Ungraded grade type
										$tlang.getString("gen.nograd")
									#elseif ($gradeScale == 2)
										## Letter grade type 
										A-F
									#elseif ($gradeScale == 3)
										## Score based grade type
										0-$assignment.getContent().getMaxGradePointDisplay()
									#elseif ($gradeScale == 4)
										## Pass/fail grade type 
										$tlang.getString("gen.pf")
									#elseif ($gradeScale == 5)
										## Grade type that only requires a check
										<img src="#imageLink("sakai/checkon.gif")" border="0" alt="Checkmark" />
									#end
								</td>
							#end
							#if ($!allowRemoveAssignment && $!view.equals('lisofass1'))
								<td headers="remove" class="screenOnly">
									<input type="checkbox" name="selectedAssignments" value="$assignmentReference" id="check_$assignmentCount" />
									<label for="check_$assignmentCount" class="skip">$tlang.getString("gen.remove")</label> 
								</td>
							#end
						</tr>
					#end
				#end
			</table>
			#if ($!allowRemoveAssignment && $!view.equals('lisofass1'))
				<p class="act">
					<input type="submit" class="active" name="eventSubmit_doDelete_confirm_assignment" value="$tlang.getString("update")" accesskey="s" />
				</p>
			#end
		</form>
	#end
</div>
