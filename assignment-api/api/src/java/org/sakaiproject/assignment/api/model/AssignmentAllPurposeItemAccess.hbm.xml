<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping default-lazy="false">
	<class name="org.sakaiproject.assignment.api.model.AssignmentAllPurposeItemAccess" table="ASSIGN_ALL_PURPOSE_ITEM_ACCESS_T">
       
        <id name="id" length="99" column="ID">
			<generator class="org.sakaiproject.id.hbm.IdGenerator"/>
		</id>
       <property name="access" type="java.lang.String" not-null="true">
            <column name="access" />
        </property>
        
         <!--  This maps to the ASSIGNMENT_MODEL_ANSWER_ITEM_T table -->
        <many-to-one name="assignmentAllPurposeItem" column="ASN_ALL_PURPOSE_ITEM_ID"
        	class="org.sakaiproject.assignment.api.model.AssignmentAllPurposeItem"
        	not-null="true" index="ASSIGN_ALL_PURPOSE_ITEM_ACCESS_I" />
    </class>
    
</hibernate-mapping>